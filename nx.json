{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"defaultBase": "v4",
	"targetDefaults": {
		"build": {
			"cache": true,
			"dependsOn": ["^build"],
			"outputs": ["{projectRoot}/dist"]
		},
		"test:lib": {
			"cache": true,
			"dependsOn": ["^build"]
		},
		"test:types": {
			"cache": true,
			"dependsOn": ["^build"]
		},
		"test:eslint": {
			"cache": true,
			"dependsOn": ["^build"]
		}
	}
}
